nekron:/nekron = >>>
    sudo(/effect player() 5 9999 255 true)
    sudo(/effect player() 10 9999 255 true)
    sudo(/effect player() 11 9999 255 true)
    sudo(/effect player() 12 9999 255 true)
    sudo(/effect player() 13 9999 1 true)
    sudo(/effect player() 16 9999 1 true)
    sudo(/effect player() 21 9999 19 true)
    sudo(/effect player() 23 9999 1 true)
    sudo(/effect player()
    sudo(/shot give player() nekronlaser)
    runas(~console,/give player() "minecraft:iron_boots 1 8:30 4:30 name:&4S&ep&4e&ce&ed&8s&0t&9e&1r_&4Boots")
    tmsg(player(),concat(color(gray),
    'I am the darkest force in the multiverse!'))
<<<
# grow into giant
grow:/grow = >>>
    sudo(/disguise giant)
<<<
shrink:/shrink = >>>
   include('../includes/changeskin.ms')
   _changeSkin(player(),'nekron') 
<<<

# Gives player strength 20 for 5 minutes
empower:/empower $plr = >>>
    @plr = $plr
    sudo(/effect @plr 5 300 20 true)
    tmsg(player(), "You have empowered ".@plr."!")
    tmsg(@plr, "You have been empowered by ". player() ."!")
<<<
