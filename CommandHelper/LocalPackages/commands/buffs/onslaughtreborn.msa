onslaughtrebornphysical:/onslaughtrebornphysical = >>>

    include('/home/minecraft/server/plugins/CommandHelper/includes/changeSkin.ms')

    sudo(/warp survival player())
    sudo(/effect player() 5 9999 20 true)
    sudo(/effect player() 10 9999 1 true)
    sudo(/effect player() 11 9999 3 true)
    sudo(/effect player() 21 9999 19 true)
    @hero = _getHeroInfo(pgroup()[0],0)
    _changeSkin(player(),@hero['sig'],@hero['val'])
    
    tmsg(player(),concat(color(red),
    'I was forgotten within the crucible of fear!'))
    
<<<

