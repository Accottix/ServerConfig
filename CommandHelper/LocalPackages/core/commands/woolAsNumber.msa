nextnumber:/nextnumber $machineNumber = >>>

    @machineNumber = $machineNumber
    # This function is relative to the command block running it
    @loc = get_command_block()
    
    @x = @loc['x'] + 1
    @y = @loc['y'] + 1
    @z = @loc['z']
    @world = 'greenfield'
    
    @block = get_block_at(@x,@y,@z,@world)
    @ar = split(':',@block)
    @num = @ar[1] + 1
    if(@num >= 10){
        @num = 0
    }
    set_block_at(@x,@y,@z,"35:".@num,@world)
    
    include('/home/minecraft/server/plugins/CommandHelper/includes/getWoolNumber.ms')
    @num = _getWoolNumber(@machineNumber)
    
    include('/home/minecraft/server/plugins/CommandHelper/includes/getHeroInfo.ms')
    @hero = _getHeroInfo(0,@num)
    
    include('/home/minecraft/server/plugins/CommandHelper/includes/updateHologram.ms')
    if(@hero['name'] != "<open slot>"){
        _updateHologram(@machineNumber, @hero['class'])
    }else{
        _updateHologram(@machineNumber, 'remove')
    }

