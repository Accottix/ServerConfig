utopialaunch:/utopialaunch $plr = >>>

    @vic = $plr
    
    @id = set_interval(500,closure(
        set_pvelocity(@vic,0,10,0) 
    ))
    export(@vic.'utopiaLaunch',@id)

    set_timeout(3950,closure(
        @id = import(@vic.'utopiaLaunch')
        clear_task(@id)
        runas(~console,'/warp thealpha '.@vic)
    ))

<<<
