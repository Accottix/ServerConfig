getnumber:/getnumber = >>>

    # This function is relative to the command block that placed it
    @loc = get_command_block()
    
    @block1locy = @loc['y'] + 4
    @block1locx = @loc['x'] + 1
    @block1locz = @loc['z'] + 1
    
    @block2locy = @loc['y'] + 4
    @block2locx = @loc['x'] + 1
    @block2locz = @loc['z']
    
    @block3locy = @loc['y'] + 4
    @block3locx = @loc['x'] + 1
    @block3locz = @loc['z'] - 1
    
    @world = 'greenfield'
    
    @block1 = get_block_at(@block1locx,@block1locy,@block1locz,@world)
    @ar = split(':',@block1)
    @num1 = @ar[1]
    
    @block2 = get_block_at(@block2locx,@block2locy,@block2locz,@world)
    @ar2 = split(':',@block2)
    @num2 = @ar2[1]
    
    @block3 = get_block_at(@block3locx,@block3locy,@block3locz,@world)
    @ar3 = split(':',@block3)
    @num3 = @ar3[1]
    
    @id = @num1.@num2.@num3
    
    include('/home/minecraft/server/plugins/CommandHelper/includes/getHeroInfo.ms')
    @hero = _getHeroInfo(0,@id)
    
    if(@hero['name'] == "<open slot>"){
        die()
    }
    
    @dnaName = @hero['name']." §e§lD§a§lN§4§lA §fSample"
    @lore = @hero['class']
    @dna = associative_array(
        'type': 373,
        'name': 'potion',
        'data': 0,
        'qty': 1,
        'meta': associative_array(
          'display': "@dnaName",
          'lore': array(@lore),
        'potions': array(
          array(
            'id': 9,
            'strength': 1,
            'seconds': 1200,
            'ambient': true,
          ),
          array(
            'id': 19,
            'strength': 1,
            'seconds': 1200,
            'ambient': true,
          ),
         ),
        ),
       );
    
    assign(@dispenserLocation,@loc)
    
    @dispenserLocation['y'] = @dispenserLocation['y'] + 1
    @dispenserLocation['z'] = @dispenserLocation['z'] + 1
    
    set_inventory_item(@dispenserLocation, 0, @dna)
    
    
    #runas(~console,/give @plr "minecraft:potion 1 0 effect:nausea power:1 duration:1200 effect:poison power:1 duration:1200 name:@hero['name'] lore:@hero['class']") 
    
<<<